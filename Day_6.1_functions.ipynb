    "# 2. `get_shop_value(shop)` -> 19\n",
    "shop = [\n",
    "    {\"name\": \"Apple\", \"quantity\": 30, \"price\": 0.50},  # 15\n",
    "    {\"name\": \"Banana\", \"quantity\": 20, \"price\": 0.20},  # 4\n",
    "]\n",
    "\n",
    "\n",
    "def get_shop_value(shop):\n",
    "    shop_value = [item[\"quantity\"] * item[\"price\"] for item in shop]\n",
    "\n",
    "    return sum(shop_value)\n",
    "\n",
    "\n",
    "get_shop_value(shop)"
